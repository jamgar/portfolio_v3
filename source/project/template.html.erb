---
directory_index: false
layout: layout
---

<div class="container">
  <h2><%= project.name %></h2>
  <% if project.image %>
    <%= image_tag project.image, alt: "#{project.image_alt}", height: '300', width: '600' %>
  <% end %>

  <h3>Description:</h3>
  <p><%= project.description %></p>
</div>
