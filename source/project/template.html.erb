---
directory_index: false
layout: layout
---
<div class="w3-main w3-animate-opacity">
  <div class="w3-container">
    <h2><%= project.name %></h2>
    <% if project.image %>
    <%= image_tag project.image, alt: "#{project.image_alt}", height: '300', width: '600' %>
    <% end %>
    <h3>Description:</h3>
    <p><%= project.description %></p>
    <p>Link: <%= project.link %></p>
  </div>
</div>
